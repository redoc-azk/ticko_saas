        <link href="{{ asset('webarch/css/webarch.css') }}" rel="stylesheet" type="text/css" />
        <!-- END CORE CSS FRAMEWORK -->

        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
